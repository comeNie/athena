<?xml version="1.0" encoding="UTF-8"?>

<!-- 参数值为空或者0都代表无意义,解析程序不会拼接 -->
<!-- taskHeight 任务高度   perPaperHeight 每页纸的高度     beginHeight 每页打印开始距纸张高度差  endHeight 每页打印结束距纸张高度差   pageInfo 是否需要换页-->
<print-config taskHeight="0" perPaperHeight="288" beginHeight="6" endHeight="6" pageInfo="true">
	
	<!-- 区域 -->
	<!-- id 区域编号,由1开始依次类推 , 对应数据库的区域字段 -->
	<!-- type 区域内容样式  sheet 表单, grid 表格 -->
	<!-- autoSr 当 type=grid的时候启用 -->
	<area id="1" type="sheet" autoSr="17" >
	<!-- 数据 -->
		<!-- parentid 父类id,对应区域id -->
		<!-- key 键值,对应打印数据键值 -->
		<!-- dataType 数据样式 TWOBYTE 汉字   BARCODE 条形码    C3/9;X1A;H7;DARK;20;38 BARCODE 只能跟着sheet类型  -->
		<!-- font 字体及样式 -->
		<!-- sr 行起始坐标 -->
		<!-- sc 列起始坐标 -->
		<!-- ve 打印数据的高度 -->
		<!-- he 打印数据的宽度 -->
		<data parentid="1" row="1" key="1-1" dataType="TWOBYTE" font="F16;P8;"  sr="7" sc="230;" ve="0;" he="0;"  value="同步备货单"/>
		<data parentid="1" row="1" key="1-22" dataType="TWOBYTE" font="F16;P8;"  sr="#sr" sc="290;" ve="0;" he="0;"  value="#replace" />
