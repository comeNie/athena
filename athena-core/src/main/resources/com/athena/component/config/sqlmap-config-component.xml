<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMapConfig      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">
<sqlMapConfig>

    <properties resource="com/athena/component/config/sqlmap.properties" />
    
	<settings cacheModelsEnabled="true" enhancementEnabled="true"
		lazyLoadingEnabled="true" maxRequests="32" maxSessions="10"
		maxTransactions="5" useStatementNamespaces="true" />
		<sqlMap resource="com/athena/component/sqlmap/component.xml" />
		<sqlMap resource="com/athena/component/sqlmap/select.xml" />
		<sqlMap resource="com/athena/component/sqlmap/dailog.xml" />
		<sqlMap resource="com/athena/component/sqlmap/interface-log.xml" />
</sqlMapConfig>
